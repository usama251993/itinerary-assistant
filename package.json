    "@angular/compiler-cli": "12.1.0",
    "@angular/language-service": "12.1.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "12.7.1",
    "@nrwl/cypress": "12.7.1",
    "@nrwl/eslint-plugin-nx": "12.7.1",
    "@nrwl/jest": "12.7.1",
