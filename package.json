{
  "name": "itinerary-assistant",
  "version": "0.4.0",
  "license": "MIT",
  "scripts": {
    "build": "nx build",
    "build:api": "nx run api:build:production --verbose",
    "postbuild:api": "ncc build ./dist/apps/api/main.js --out ./dist/apps/api --minify",
    "build:client": "nx run frontend-client:build:production",
    "build:client:stats:dev": "nx run frontend-client:build:development --stats-json",
    "build:client:stats:prod": "nx run frontend-client:build:production --stats-json --verbose",
    "build:admin": "nx run frontend-admin:build:production",
    "build:admin:stats:dev": "nx run frontend-admin:build:development --stats-json",
    "build:admin:stats:prod": "nx run frontend-admin:build:production --stats-json --verbose",
    "start": "nx run-many --target=serve --all=true",
    "start:api": "nx run api:serve",
    "start:client": "nx run frontend-client:serve:development",
    "start:admin": "nx run frontend-admin:serve:development",
    "analyze:source-map:client": "source-map-explorer dist/apps/frontend/client/*.{js,css} --no-border-checks --only-mapped --exclude-source-map",
    "analyze:source-map:admin": "source-map-explorer dist/apps/frontend/admin/*.{js,css} --no-border-checks --only-mapped --exclude-source-map",
    "test": "nx test"
  },
  "private": true,
