{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "dom",
      "es2018"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "paths": {
      "@ia-core/*": [
        "app/modules/ia-core/*"
      ],
    },
    "sourceMap": true,
    "target": "es2015"
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
