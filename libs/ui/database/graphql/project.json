{
  "projectType": "library",
  "sourceRoot": "libs/ui/database/graphql/src",
  "prefix": "itinerary-assistant",
  "targets": {
    "generate": {
      "builder": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx graphql-codegen --config configs/graphql-codegen/codegen.yml"
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ui/database/graphql"],
      "options": {
        "jestConfig": "libs/ui/database/graphql/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ui/database/graphql/**/*.ts",
          "libs/ui/database/graphql/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
