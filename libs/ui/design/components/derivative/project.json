{
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ui/design/components/derivative/src",
  "prefix": "itinerary-assistant",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/ui/design/components/derivative"],
      "options": {
        "project": "libs/ui/design/components/derivative/ng-package.json",
        "tailwindConfig": "libs/ui/design/components/derivative/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ui/design/components/derivative/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ui/design/components/derivative/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ui/design/components/derivative"],
      "options": {
        "jestConfig": "libs/ui/design/components/derivative/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ui/design/components/derivative/src/**/*.ts",
          "libs/ui/design/components/derivative/src/**/*.html"
        ]
      }
    }
  },
  "tags": [
    "species:ui-design-components-derivative",
    "genus:library",
    "family:ui"
  ]
}
