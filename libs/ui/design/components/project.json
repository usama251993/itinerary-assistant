{
  "projectType": "library",
  "root": "libs/ui/design/components",
  "sourceRoot": "libs/ui/design/components/src",
  "prefix": "itinerary-assistant",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/ui/design/components"],
      "options": {
        "project": "libs/ui/design/components/ng-package.json",
        "tailwindConfig": "libs/ui/design/components/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ui/design/components/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ui/design/components/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ui/design/components"],
      "options": {
        "jestConfig": "libs/ui/design/components/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ui/design/components/src/**/*.ts",
          "libs/ui/design/components/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["species:ui-design-components", "genus:library", "family:ui"]
}
