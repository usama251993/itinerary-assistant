{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ui/design/icons/src",
  "prefix": "itinerary-assistant",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/ui/design/icons"],
      "options": {
        "project": "libs/ui/design/icons/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ui/design/icons/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ui/design/icons/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ui/design/icons"],
      "options": {
        "jestConfig": "libs/ui/design/icons/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ui/design/icons/src/**/*.ts",
          "libs/ui/design/icons/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["species:ui-shared-icons", "genus:library", "family:ui"]
}
