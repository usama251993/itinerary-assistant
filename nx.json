{
  "npmScope": "itinerary-assistant",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ],
        "accessToken": "MWNiY2EzZmUtZTFmYS00YWY3LWE3ZWUtNmQ1MjlmMDFiMmJlfHJlYWQtd3JpdGU=",
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": [
        "app:api"
      ]
    },
    "ui": {
      "tags": [
        "app:ui"
      ]
    },
    "ui-e2e": {
      "tags": [
        "test:ui-e2e"
      ],
      "implicitDependencies": [
        "ui"
      ]
    },
    "api-core": {
      "tags": [
        "lib:api-core"
      ]
    },
    "api-database-entities": {
      "tags": [
        "lib:api-database-entities"
      ]
    },
    "api-database-typeorm": {
      "tags": [
        "lib:api-database-typeorm"
      ]
    },
    "api-features-trip": {
      "tags": [
        "lib:api-features-trip"
      ]
    },
    "shared-data": {
      "tags": [
        "lib:shared-data"
      ]
    },
    "shared-styles": {
      "tags": [
        "lib:shared-styles"
      ]
    },
    "ui-core": {
      "tags": [
        "lib:ui-core"
      ]
    },
    "ui-features-trip": {
      "tags": [
        "lib:ui-features-trip"
      ]
    },
    "ui-layout": {
      "tags": [
        "lib:ui-layout"
      ]
    },
    "ui-shared-elements": {
      "tags": [
        "lib:ui-shared-elements"
      ]
    },
    "ui-shared-icons": {
