{
  "npmScope": "itinerary-assistant",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "MWNiY2EzZmUtZTFmYS00YWY3LWE3ZWUtNmQ1MjlmMDFiMmJlfHJlYWQtd3JpdGU=",
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": ["app:api"]
    },
    "api-core": {
      "tags": ["lib:api-core"]
    },
    "api-trip": {
      "tags": ["lib:api-trip"]
    },
    "icon": {
      "tags": ["lib:ui-icon"]
    },
    "layout": {
      "tags": ["lib:ui-layout"]
    },
    "material": {
      "tags": ["lib:ui-material"]
    },
    "shared": {
      "tags": ["shared"]
    },
    "tailwind": {
      "tags": ["lib:tailwind"]
    },
    "ui": {
      "tags": ["app:ui"]
    },
    "ui-core": {
      "tags": ["lib:ui-core"]
    },
    "ui-e2e": {
      "tags": ["test:ui-e2e"],
      "implicitDependencies": ["ui"]
    }
  }
}
